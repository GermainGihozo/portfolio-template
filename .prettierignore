# Ignore all files in the node_modules directory
node_modules

# Ignore all files in the build directory
build

# Ignore all files in the dist directory
dist

# Ignore all files with a .min.js extension
*.min.js

# Ignore all files with a .min.css extension
*.min.css

# Ignore all files with a .map extension
*.map

# Ignore all files with a .log extension
*.log